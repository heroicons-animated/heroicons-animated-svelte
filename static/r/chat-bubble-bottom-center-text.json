RIANTS blink) */\n.text-line {\n  stroke-dasharray: 1;\n  stroke-dashoffset: 0;\n  opacity: 1;\n}\n\n.text-line.text-line-animate {\n  animation: text-line-blink 0.8s ease-out forwards;\n}\n\n.text-line-2.text-line-animate {\n  animation-delay: 0.1s;\n}\n\n@keyframes text-line-blink {\n  0% {\n    stroke-dashoffset: 0;\n    opacity: 1;\n  }\n  37.5% {\n    stroke-dashoffset: 1;\n    opacity: 0;\n  }\n  75% {\n    stroke-dashoffset: 0;\n    opacity: 1;\n  }\n  100% {\n    stroke-dashoffset: 0;\n    opacity: 1;\n  }\n}\n</style>\n",
