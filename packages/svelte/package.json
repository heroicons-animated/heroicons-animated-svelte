{
  "name": "@heroicons-animated/svelte",
  "version": "1.2.0",
  "description": "Animated Heroicons for Svelte, built with CSS animations",
  "author": "Aniket-508",
  "license": "MIT",
  "homepage": "https://svelte.heroicons-animated.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/heroicons-animated/heroicons-animated-svelte",
    "directory": "packages/svelte"
  },
  "bugs": {
    "url": "https://github.com/heroicons-animated/heroicons-animated-svelte/issues"
  },
  "keywords": [
    "heroicons",
    "svelte",
    "svelte5",
    "icons",
    "animated",
    "css-animations"
  ],
  "sideEffects": false,
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/icons/*.d.ts",
      "svelte": "./dist/icons/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/icons/*.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "*.js",
    "*.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "svelte-package && node scripts/generate-proxies.mjs",
    "dev": "svelte-package --watch",
    "type-check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.4",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "svelte": "^5.20.0",
    "svelte-check": "^4.0.7",
    "typescript": "catalog:",
    "vite": "catalog:"
  }
}
