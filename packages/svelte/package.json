{
  "name": "@heroicons-animated/svelte",
  "version": "0.1.0",
  "description": "Animated Svelte icons built with CSS animations",
  "author": "Aniket-508",
  "license": "MIT",
  "homepage": "https://heroicons-animated.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aniket-508/heroicons-animated",
    "directory": "packages/svelte"
  },
  "keywords": [
    "heroicons",
    "svelte",
    "svelte5",
    "icons",
    "animated",
    "css-animations"
  ],
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./icons/*": {
      "svelte": "./dist/icons/*.js",
      "types": "./dist/icons/*.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "svelte-package",
    "dev": "svelte-package --watch",
    "type-check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@sveltejs/package": "^2.3.4",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "svelte": "^5.20.0",
    "svelte-check": "^4.0.7",
    "typescript": "catalog:",
    "vite": "catalog:"
  }
}
